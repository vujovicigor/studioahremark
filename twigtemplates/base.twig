     <script>
        $(window).scroll(function() {
            var theta = $(window).scrollTop() / 60 % Math.PI;
            $('#logo').css({ transform: 'rotate(' + theta + 'rad)' });
        });
        
        var owl = $('.owl-carousel');
        owl.owlCarousel({
            items:1,
            loop:true,
            autoplay:true,
            autoplayTimeout:5000,
            autoplayHoverPause:true,
            nav:false,
            dots:true,
            animateOut: 'fadeOut'
        });
